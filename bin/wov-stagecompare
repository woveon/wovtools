#!/usr/bin/env bash

# --------------------------------------------------------------------- 
# Load wov-env if not already loaded
# --------------------------------------------------------------------- 
if [ "${WOV_VERSION}" != "" ]; then
  args=( "$@" )
  set ""
  . wov-env
  set -- "${args[@]}"
fi

# --------------------------------------------------------------------- 
# Check that they align
# --------------------------------------------------------------------- 
# echo "WOV_STAGE : $WOV_STAGE"
if [ "$WOV_KSTAGE" == "" ]; then
  echo "ERROR: unknown WOV_KSTAGE";
  exit 1
elif [ "$WOV_GSTAGE" == "" ]; then
  echo "ERROR: unknown WOV_KSTAGE";
  exit 2
elif [ "$WOV_KSTAGE" != "$WOV_GSTAGE" ]; then
  echo "ERROR: STAGE miss-match. (WOV_KSTAGE=${WOV_KSTAGE}) (WOV_GSTAGE=${WOV_GSTAGE}).";
  exit 3
fi

